    <embed type="application/x-shockwave-flash" src="../../../../../../aRzModule/all-information-of-tawian/aRzImages/worldmap.swf" menu="true" loop="loop" play="true" pluginspage="http://www.macromedia.com/go/getflashplayer" height="380" width="570"></embed>
